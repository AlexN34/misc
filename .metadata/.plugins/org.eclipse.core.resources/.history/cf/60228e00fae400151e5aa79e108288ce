package tutwk2;

public class Employee {
	private String name;
	private int salary;
	public Employee (String name, int salary) {
		this.name = name;
		this.salary = salary;
	}
	
	public String getName () {
		return this.name;
	}
	
	public int getSalary () {
		return this.salary;
	}
	public void setName (String name) {
		this.name = name;
	}
	
	public void setSalary (int salary) {
		this.salary = salary;
	}
	
	public String toString () {
		return result = "{ name =" + this.name + "," + this.salary + "}";
		return result;
	}
	
	public _ equals() {
		
	}
	
	public _ clone() {
		
	}
}

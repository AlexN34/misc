import java.util.ArrayList;
import java.util.Map;

public class BFSGraph implements Graph<String> {
	Map<Vertex<String>, ArrayList<Vertex<String>>> vertices;
//	Vertex[] adjLists
	@Override
	public void addVertex (Vertex<String> v) {
		this.vertices.put(v, new ArrayList<Vertex<String>>());
	}
	@Override
	public void removeVertex (Vertex<String> v) {
		this.vertices.remove(v);
		
	}
	
	 public Map<Vertex<String>, ArrayList<Vertex<String>>> getVertices() {
		return this.vertices;
	}
	public void setVertices(Map<Vertex<String>, ArrayList<Vertex<String>>> vertices) {
		this.vertices = vertices;
	}

	public BFSGraph(ArrayList<Vertex<String>> vertices) {
		super();
		this.vertices = vertices;
	}

	public BFSGraph() {
		super();
		this.vertices = new Map<Vertex<String>, ArrayList<Vertex<String>>>();
	}
	@Override
	public void addEdge(Edge<String> e) {
		// TODO Auto-generated method stub
		
	}
	@Override
	public void removeEdge(Edge<String> e) {
		// TODO Auto-generated method stub
		
	}

}
